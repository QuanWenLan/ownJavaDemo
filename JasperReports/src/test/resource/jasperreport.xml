<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="dailyClientTradeStatisticsReport" class="com.ettrade.ngbss.jasperreport.common.JasperReportGenerator">
        <property name="fileName" value="dailyClientTradeStatisticsReport" />
        <property name="resourcePath"
                  value="dailyClientTradeStatisticsReport" />
        <property name="generator">
            <bean class="com.ettrade.ngbss.jasperreport.data.generator.clientacreport.DailyClientTradeStatisticsReportGenerator" />
        </property>
    </bean>


    <bean id="ReportService" class="com.ettrade.ngbss.jasperreport.service.ReportService">
<!--        <constructor-arg index="0" value="ReportService" />-->
        <property name="reportGeneratorMap">
            <map key-type="com.ettrade.ngbss.jasperreport.common.JasperReportId">
                <entry key="DailyClientTradeStatisticsReport" value-ref="dailyClientTradeStatisticsReport" />
            </map>
        </property>
    </bean>
</beans>
